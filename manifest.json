{
    "manifest_version": 3,
    "name": "Shopify Orbitvu Importer",
    "version": "1.0",
    "description": "Import Orbitvu project into Shopify",
    "content_scripts": [
      {
        "js": ["scripts/content.js"],
        "matches": ["https://admin.shopify.com/store/*/products/new"]
      }
    ],
    "icons": {
      "32": "icons/icon-32.png"
    },
    "permissions": [
      "contextMenus",
      "storage"
    ],
    "background": {
      "service_worker": "scripts/menu.js"
    }
  }
